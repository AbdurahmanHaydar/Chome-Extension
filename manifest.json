{
  "name": "Do!",
  "description": "Write Things Todo",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["storage"],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "background": {
    "scripts": ["jquery.js"],
    "persistent": false
  },

  "browser_action": {
    "default_title": "notify",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "me.png"
  }
}
